let S := string::buffer()

S:write("fun(X) X + {\n\t\"A1\" is 1")
for I in 2 .. 100 do
	S:write(',\n\t\"A{I}\" is {I}')
end
S:write("\n}")

let C := compiler(globals())
let P := parser()
P:source("test", 1)
P:input(S:rest)

let F1 := P:compile(C)()

let E := cbor::encode(F1)

print(base16::encode(E), "\n")

let F2 := cbor::decode(E)

print(F2({}))
